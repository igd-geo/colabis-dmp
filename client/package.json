{
  "name": "dmp-client",
  "version": "1.0.0",
  "description": "Web-based client for the Colabis Data Management Platform",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "clean": "npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --progress --profile --config config/webpack.dev.js",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "NODE_ENV='prod' webpack --progress --profile --bail  --config config/webpack.prod.js",
    "lint": "npm run tslint 'src/**/*.ts'",
    "pretest": "npm run lint",
    "test": "karma start",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:igd-geo/colabis-dmp.git"
  },
  "author": "Ivo Senner <ivo.senner@igd.fraunhofer.de>",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "^2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/platform-server": "2.1.1",
    "@angular/router": "3.1.1",
    "@swimlane/ngx-datatable": "^4.0.0",
    "@types/xml2js": "^0.4.2",
    "angular2-modal": "2.0.1",
    "angular2-notifications": "^0.4.49",
    "angular2-tree-component": "^2.6.1",
    "core-js": "^2.4.1",
    "dragula": "^3.7.2",
    "ie-shim": "^0.1.0",
    "jquery": "^3.1.1",
    "ng2-dragula": "^1.2.1",
    "ng2-tag-input": "0.7.4",
    "rxjs": "^5.0.1",
    "uikit": "^2.27.2",
    "xml2js": "^0.4.19",
    "zone.js": "~0.6.17"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.2.34",
    "@types/jquery": "^2.0.34",
    "@types/node": "^6.0.38",
    "@types/selenium-webdriver": "^2.44.29",
    "@types/source-map": "^0.1.27",
    "@types/uglify-js": "^2.0.27",
    "@types/uikit": "^2.27.2",
    "@types/webpack": "^1.12.34",
    "angular2-template-loader": "^0.6.0",
    "assets-webpack-plugin": "^3.4.0",
    "awesome-typescript-loader": "^2.2.1",
    "babel-loader": "6.2.7",
    "codelyzer": "~0.0.28",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "less-loader": "^2.2.3",
    "node-sass": "^3.11.2",
    "postcss-loader": "1.1.1",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.3",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "1.1.1",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.4.5",
    "typescript": "^2.0.6",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.15.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
